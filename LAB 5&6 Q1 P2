#include <iostream>
using namespace std;

int main() {
    string password;
    bool correctpassword=true;
    while(correctpassword){
        cout <<"enter the password:";
        cin >> password;
        if(password=="phyton123") {
            cout <<"access granted" <<endl;
            correctpassword=false;
        }else{
            cout <<"access denied" <<endl;

        }
    }

    return 0;
}
